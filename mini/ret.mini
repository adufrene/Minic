fun foo(int i) int      # standard case
{
   return i;
}

fun bar(int i) int      # control flow case
{
   if (i > 0)
   {
      return 1;
   }
   else
   {
      if (i < 0)
      {
         return -1;
      }
      else
      {
         return 0;
      }
   }
}

fun main() int {
    int x, y, z;
    x = 3;
    y = foo(x);
    z = bar(y);
#    print x endl;
#    print y endl;
#    print z endl;
    return x * y / z;
}
